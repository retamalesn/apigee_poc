<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-XML">
    <DisplayName>AM-XML</DisplayName>
    <Set>
        <Payload contentType="text/xml">
            <travelmode>{directionsresponse.travelmode}</travelmode>
            <duration>{directionsresponse.duration}</duration>
            <timeunit>{directionsresponse.timeunit}</timeunit>
        </Payload>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>